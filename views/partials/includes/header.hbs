                  <button class="btn navBtn" type="button" name="logoutBtn" id="logoutBtn">Logout</button>
                </li>
                <li>
                  <button class="btn navBtn" type="button" name="showProfile" id="showProfile">Show Profile</button>
                </li>
              {{/unless}}
